{
    "tabWidth": 4,
    "useTabs": false,
    "svelteSortOrder": "options-scripts-markup-styles",
    "svelteIndentScriptAndStyle": true,
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-svelte"
    ],
    "tailwindStylesheet": "./src/routes/layout.css",
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
